{
  "port": "EZMESURE_PORT",
  "elasticsearch": {
    "scheme": "EZMESURE_ELASTIC_SCHEME",
    "port": "EZMESURE_ELASTIC_PORT",
    "host": "EZMESURE_ELASTIC_HOST",
    "url": "EZMESURE_ELASTIC_URL",
    "user": "EZMESURE_ELASTIC_USER",
    "password": "ELASTICSEARCH_PASSWORD",
    "apiVersion": "EZMESURE_ELASTIC_VERSION",
    "syncSchedule": "EZMESURE_ELASTIC_SYNC_SCHEDULE"
  },
  "kibana": {
    "username": "KIBANA_USERNAME",
    "password": "KIBANA_PASSWORD",
    "port": "EZMESURE_KIBANA_PORT",
    "host": "EZMESURE_KIBANA_HOST",
    "url": "EZMESURE_KIBANA_URL",
    "dateFormat": "EZMESURE_KIBANA_DATE_FORMAT",
    "syncSchedule": "EZMESURE_KIBANA_SYNC_SCHEDULE"
  },
  "redis": {
    "host": "EZMESURE_REDIS_HOST",
    "port": "EZMESURE_REDIS_PORT",
    "password": "EZMESURE_REDIS_PASSWORD"
  },
  "ezreeport": {
    "host": "EZMESURE_REPORT_HOST",
    "port": "EZMESURE_REPORT_PORT",
    "syncSchedule": "EZMESURE_REPORT_SYNC_SCHEDULE",
    "apiKey": "EZMESURE_REPORT_ADMIN_KEY"
  },
  "smtp": {
    "host": "EZMESURE_SMTP_HOST",
    "port": "EZMESURE_SMTP_PORT",
    "secure": { "__name": "EZMESURE_SMTP_SECURE", "__format": "boolean" },
    "ignoreTLS": { "__name": "EZMESURE_SMTP_IGNORE_TLS", "__format": "boolean" },
    "tls": {
      "rejectUnauthorized": { "__name": "EZMESURE_SMTP_REJECT_UNAUTHORIZED", "__format": "boolean" }
    }
  },
  "notifications": {
    "cron": "EZMESURE_NOTIFICATIONS_CRON",
    "sender": "EZMESURE_NOTIFICATIONS_SENDER",
    "recipients": "EZMESURE_NOTIFICATIONS_RECIPIENTS",
    "supportRecipients": "EZMESURE_NOTIFICATIONS_SUPPORT_RECIPIENTS",
    "sendEmptyActivity": { "__name": "EZMESURE_SEND_EMPTY_ACTIVITY", "__format": "boolean" }
  },
  "auth": {
    "secret": "EZMESURE_AUTH_SECRET",
    "cookie": "EZMESURE_AUTH_COOKIE",
    "oidc": {
      "client": {
        "id": "EZMESURE_OIDC_CLIENT_ID",
        "secret": "EZMESURE_OIDC_CLIENT_SECRET"
      },
      "uris": {
        "discovery": "EZMESURE_OIDC_DISCOVERY_URI",

        "issuer": "EZMESURE_OIDC_ISSUER_URI",
        "authorization": "EZMESURE_OIDC_AUTH_URI",
        "token": "EZMESURE_OIDC_TOKEN_URI",
        "introspection": "EZMESURE_OIDC_INTROSPECTION_URI",
        "userinfo": "EZMESURE_OIDC_USERINFO_URI"
      },
      "scopes": { "__name": "EZMESURE_OIDC_SCOPES", "__format": "json" }
    }
  },
  "admin": {
    "username": "EZMESURE_ADMIN_USERNAME",
    "fullName": "EZMESURE_ADMIN_FULLNAME",
    "password": "EZMESURE_ADMIN_PASSWORD",
    "email": "EZMESURE_ADMIN_EMAIL"
  },
  "storage": {
    "path": "EZMESURE_STORAGE_PATH"
  },
  "ezpaarse": {
    "upload": {
      "bulkSize": { "__name": "EZMESURE_EZPAARSE_UPLOAD_BULK_SIZE", "__format": "number" },
      "bulkMaxTries": { "__name": "EZMESURE_EZPAARSE_UPLOAD_BULK_MAX_TRIES", "__format": "number" },
      "bulkBaseRetryDelay": { "__name": "EZMESURE_EZPAARSE_UPLOAD_BULK_BASE_RETRY_DELAY", "__format": "number" }
    }
  },
  "jobs": {
    "harvest": {
      "concurrency": "EZMESURE_HARVEST_CONCURRENCY",
      "maxDeferrals": "EZMESURE_MAX_DEFERRALS",
      "deferralBackoffDuration": "EZMESURE_DEFERRAL_BACKOFF_DURATION",
      "busyBackoffDuration": "EZMESURE_BUSY_BACKOFF_DURATION",
      "cancelSchedule": "EZMESURE_HARVEST_CANCEL_SCHEDULE"
    }
  },
  "counter": {
    "defaultHarvestedReports": {
      "__name": "EZMESURE_DEFAULT_HARVESTED_REPORTS",
      "__format": "json"
    },
    "clean": {
      "schedule": "EZMESURE_CLEAN_COUNTER_SCHEDULE",
      "maxDayAge": "EZMESURE_COUNTER_MAX_AGE"
    },
    "deleteSushi": {
      "schedule": "EZMESURE_DELETE_SUSHI_COUNTER_SCHEDULE",
      "maxDayAge": "EZMESURE_DELETE_SUSHI_COUNTER_FOLLOW_INTERVAL"
    },
    "alerts": {
      "harvestedButUnsupported": {
        "schedule": "EZMESURE_SUSHI_ALERT_HARVESTED_BUT_UNSUPPORTED_SCHEDULE"
      },
      "endpoint": {
        "schedule": "EZMESURE_SUSHI_ALERT_ENDPOINT_SCHEDULE"
      }
    }
  },
  "testUsers": {
    "lifespan": {
      "max": "EZMESURE_TEST_USERS_MAX_LIFESPAN",
      "default": "EZMESURE_TEST_USERS_DEFAULT_LIFESPAN"
    },
    "clean": {
      "schedule": "EZMESURE_TEST_USERS_CLEAN_SCHEDULE"
    }
  },
  "depositors": {
    "index": "EZMESURE_DEPOSITORS_INDEX",
    "spreadsheetId": "EZMESURE_DEPOSITORS_SPREADSHEET",
    "cron": "EZMESURE_DEPOSITORS_CRON"
  },
  "cypher": {
    "secret": "EZMESURE_ENCRYPTION_SECRET"
  },
  "logs": {
    "app": {
      "Console": {
        "level": "EZMESURE_LOG_LEVEL"
      }
    }
  },
  "appName": "EZMESURE_APP_NAME",
  "publicUrl": "EZMESURE_PUBLIC_URL"
}
